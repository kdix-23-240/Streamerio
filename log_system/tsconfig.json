{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ES2022",
    "lib": ["es2022","dom"],
    "moduleResolution": "bundler",
    "moduleDetection": "force",
    "strict": true,
    "noEmit": true,
    "types": ["@cloudflare/workers-types"],
    "allowImportingTsExtensions": true,
    "verbatimModuleSyntax": true
  },
  "include": ["src/**/*"]
}
